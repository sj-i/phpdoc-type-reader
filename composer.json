{
  "name": "sj-i/phpdoc-type-reader",
  "description": "A library for reading types from phpdoc",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "php",
    "type",
    "phpdoc",
    "generics"
  ],
  "authors": [
    {
      "name": "sji",
      "homepage": "https://twitter.com/sji_ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhpDocTypeReader\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDocTypeReader\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^4.6",
    "squizlabs/php_codesniffer": "^3.5",
    "mockery/mockery": "^1.4",
    "psalm/plugin-mockery": "^0.7.0"
  }
}