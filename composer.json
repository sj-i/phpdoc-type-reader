{
  "name": "sj-i/phpdoc-type-reader",
  "description": "A library for reading types from phpdoc",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "php",
    "type",
    "phpdoc",
    "generics"
  ],
  "authors": [
    {
      "name": "sji",
      "homepage": "https://twitter.com/sji_ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhpDocTypeReader\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDocTypeReader\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "phpstan/phpdoc-parser": "1.2.x",
    "nikic/php-parser": "^4.10"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.9",
    "vimeo/psalm": "4.10.0",
    "squizlabs/php_codesniffer": "3.6.0",
    "mockery/mockery": "1.4.4",
    "psalm/plugin-mockery": "0.9.0",
    "php-coveralls/php-coveralls": "9999999-dev"
  }
}
