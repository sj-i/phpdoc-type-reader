{
  "name": "sj-i/phpdoc-type-reader",
  "description": "A library for reading types from phpdoc",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "php",
    "type",
    "phpdoc",
    "generics"
  ],
  "authors": [
    {
      "name": "sji",
      "homepage": "https://twitter.com/sji_ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhpDocTypeReader\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDocTypeReader\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "phpstan/phpdoc-parser": "^1.13",
    "nikic/php-parser": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.10",
    "squizlabs/php_codesniffer": "3.6.2",
    "mockery/mockery": "1.6.12",
    "php-coveralls/php-coveralls": "2.7.0",
    "psalm/phar": "4.10.0"
  }
}
